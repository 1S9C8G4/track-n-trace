{
	"env" : {
		"server" : {
			"presets": [
			  "es2015",
			  "es2016",
			  "es2017"
			],
			"plugins": [
			  "transform-runtime",
			  "transform-react-jsx",
				["css-modules-transform", {
	      	"generateScopedName": "[hash:10]",
	       	"extensions": [".css"]
				}]
			]
		},
		"client" : {
			"presets": [
			  "es2015",
			  "es2016",
			  "es2017"
			],
			"plugins": [
			  "transform-runtime",
			  "transform-react-jsx"
			]
		}
	}
}
